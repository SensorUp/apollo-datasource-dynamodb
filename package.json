{
  "name": "@sensorup/apollo-datasource-dynamodb",
  "version": "1.2.0",
  "description": "Apollo DataSource framework for AWS DynamoDB",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "co:login": "aws codeartifact login --tool npm --domain sensorup --domain-owner 714588172827 --region us-west-2 --repository su-npm",
    "check-types": "tsc",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --list-different --ignore-path .gitignore || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "clean-report": "rimraf ./coverage",
    "test": "npm run clean-report && jest -c jest.config.js --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prebuild": "rimraf ./lib ./lib-esm",
    "build": "tsc && tsc -m es6 --outDir lib-esm",
    "prepare": "npm run co:login && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format:prettier && git add -A src",
    "postversion": "git push && git push --tags",
    "version:major": "yarn version --major --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:minor": "yarn version --minor --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:patch": "yarn version --patch --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:beta": "yarn version --prerelease --preid beta --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:hotfix": "yarn version --prerelease --preid hotfix --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:rc": "yarn version --prerelease --preid rc --no-commit-hooks --no-git-tag-version && yarn version:tag",
    "version:tag": "git commit -a -m v$npm_package_version --no-verify && git tag -a v$npm_package_version -m v$npm_package_version && git push --follow-tags && echo \"Pushed version v$npm_package_version!\""
  },
  "homepage": "https://github.com/sensorup/apollo-datasource-dynamodb",
  "repository": {
    "type": "git",
    "url": "https://github.com/sensorup/apollo-datasource-dynamodb"
  },
  "license": "MIT",
  "keywords": [
    "apollo",
    "graphql",
    "dynamodb",
    "datasource",
    "data source"
  ],
  "files": [
    "/lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.154.0",
    "@aws-sdk/lib-dynamodb": "^3.154.0",
    "@aws-sdk/node-http-handler": "^3.127.0",
    "@aws-sdk/util-dynamodb": "^3.154.0",
    "apollo-datasource": "^3.3.2",
    "apollo-server-caching": "^3.3.0",
    "apollo-server-errors": "^3.3.1",
    "dataloader": "^2.1.0",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.8.8",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@types/jest": "^28.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "jest": "^28.0.0",
    "jest-dynalite": "^3.6.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.0.0",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^1.7.2"
  },
  "directories": {
    "lib": "lib"
  }
}
